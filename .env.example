GEMINI_API_KEY=your_gemini_api_key

# Optional: public backend URL used by frontend in production.
# Example: VITE_API_BASE_URL=https://horse-api.onrender.com
VITE_API_BASE_URL=

# Optional: frontend proxy target for /api in vite dev server.
API_PROXY_TARGET=http://127.0.0.1:8787

# Optional: backend server host/port.
API_HOST=0.0.0.0
API_PORT=8787

# Optional: Gemini model used by backend proxy.
GEMINI_MODEL=gemini-2.5-flash

# Optional: restrict browser origins when backend is public.
# Example: ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
ALLOWED_ORIGINS=
